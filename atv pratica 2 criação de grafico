      "cell_type": "code",
      "source": [
        "from matplotlib import pyplot as plt\n",
        "# Menu e valores de y para calculo da equação\n",
        "def menu (x, a, b, c):\n",
        "    y =  a*x + b*x - c\n",
        "    return y\n",
        "# definição de valores do x\n",
        "x1 = 5\n",
        "x2 = 7\n",
        "x3 = 9\n",
        "#entrada de dados dos valores para a, b, c\n",
        "a = int(input('Digite o antepenultimo numero do seu RU: '))\n",
        "b = int(input('Digite o penultimo numero do seu RU: '))\n",
        "c = int(input('Digite o ultimo numero do seu RU: '))\n",
        "# informando titulo e folha quadriculada no gráfico\n",
        "plt.title('y =  a*x + b*x - c')\n",
        "plt.grid()\n",
        "#plotagem dos resultados dos eixos X e Y\n",
        "plt.plot(x1,menu (x1, a, b, c), marker = \"o\", markersize = 12, markerfacecolor = 'green')\n",
        "plt.plot(x2,menu (x2, a, b, c), marker = \"o\", markersize = 12, markerfacecolor = 'blue')\n",
        "plt.plot(x3,menu (x3, a, b, c), marker = \"o\", markersize = 12, markerfacecolor = 'red')\n",
        "# Informativo de legenda do gráfico\n",
        "plt.legend([f'x={x1} y={menu (x1, a, b, c)}', f'x={x2} y={menu (x2, a, b, c)}',f'x={x3} y={menu (x3, a, b, c)}'])\n",
        "\n",
        "plt.show()"
